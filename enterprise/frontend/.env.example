# Frontend Environment Variables

# API Configuration
VITE_API_URL=https://api.sms-sm.health
VITE_GRAPHQL_URL=https://api.sms-sm.health/graphql
VITE_SOCKET_URL=https://api.sms-sm.health

# Authentication
VITE_AUTH_DOMAIN=sms-sm.auth0.com
VITE_AUTH_CLIENT_ID=your_auth0_client_id
VITE_AUTH_AUDIENCE=https://api.sms-sm.health

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_VIDEO_CALLS=true
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_MOBILE_APP=true

# Third-party Services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXX-X
VITE_HOTJAR_ID=your_hotjar_id
VITE_INTERCOM_APP_ID=your_intercom_app_id

# CDN and Assets
VITE_CDN_URL=https://cdn.sms-sm.health
VITE_ASSETS_URL=https://assets.sms-sm.health

# Maps and Location
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Payment and Billing
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_xxxxxxxxx

# Real-time Features
VITE_PUSHER_APP_ID=your_pusher_app_id
VITE_PUSHER_KEY=your_pusher_key
VITE_PUSHER_SECRET=your_pusher_secret
VITE_PUSHER_CLUSTER=us2

# Security
VITE_CSP_NONCE=random_nonce_value
VITE_APP_SECRET=your_app_secret_key

# Build Information
VITE_APP_VERSION=2.0.0
VITE_BUILD_NUMBER=1234
VITE_COMMIT_HASH=abc123def456

# Internationalization
VITE_DEFAULT_LOCALE=en-US
VITE_SUPPORTED_LOCALES=en-US,pt-BR,es-ES,fr-FR,de-DE,it-IT,ja-JP,ko-KR,zh-CN,ar-SA

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_STRATEGY=networkFirst
VITE_API_TIMEOUT=30000

# Debugging
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info